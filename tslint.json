{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "quotemark": [true, "single", "jsx-double"],
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-console": false,
        "trailing-comma": false,
        "variable-name": {
            "options": [
                "allow-trailing-underscore"
            ]
        }
    },
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-console": false,
        "trailing-comma": false,
        "variable-name": ["allow-leading-underscore"]
    },
    "rulesDirectory": []
}